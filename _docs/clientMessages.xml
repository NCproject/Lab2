<!-- Sign in request -->
<envelope>
	<header>
		<action>
			SIGN
		</action>		
	</header>
	<body>		
		<login>
			login
		</login>
		<password>
			password
		</password>
	</body>
</envelope>

<!-- Show filters request -->
<envelope>
	<header>
		<action>
			SHOW_FILTERS
		</action>		
	</header>
	<body>	
	</body>
</envelope>

<!-- Search students request -->
<envelope>
	<header>
		<action>
			SEARCH_STUDENTS
		</action>		
	</header>
	<body>	
		<faculty>
			facultyId
		</faculty>
		<group>
			groupId
		</group>
		<searchText>
			searchText
		</searchText>
	</body>
</envelope>

<!-- Add student request -->
<envelope>
	<header>
		<action>
			ADD_STUDENT
		</action>		
	</header>
	<body>			
		<group>
			groupId
		</group>
		<studentName>
			firstName
		</studentName>
		<studentLastname>
			lastName
		</studentLastname>
		<enrolledDate>
			date
		</enrolledDate>	
	</body>
</envelope>

<!-- Change student request -->
<envelope>
	<header>
		<action>
			CHANGE_STUDENT
		</action>		
	</header>
	<body>			
		<id>
			studentId
		</id>
		<studentName>
			firstName
		</studentName>
		<studentLastname>
			lastName
		</studentLastname>
		<enrolledDate>
			date
		</enrolledDate>	
	</body>
</envelope>

<!-- Remove student request -->
<envelope>
	<header>
		<action>
			REMOVE_STUDENT
		</action>		
	</header>
	<body>			
		<id>
			studentId
		</id>		
	</body>
</envelope>

