<!-- Show response -->
<envelope>
	<header>
		<action>
			SHOW
		</action>		
	</header>
	<body>	
		<students>
			<student>
				<studentID>
					id 
				</studentID>
				<studentName>
					firstName
				</studentName>
				<studentLastname>
					lastName
				</studentLastname>
				<enrolledDate>
					date
				</enrolledDate>	
			</student>
		</students>
	</body>
</envelope>

<!-- Update response -->
<envelope>
	<header>
		<action>
			UPDATE
		</action>		
	</header>
	<body>
		<groups>
			<group>
				<fakulty>
					faculty
				</fakulty>
				<name>
					groupName
				</name>
			</group>
		</groups>	
	</body>
</envelope>

<!-- Result response -->
<envelope>
	<header>
		<action>
			Success || Exception
		</action>		
	</header>
	<body>
		<!-- in case of exception -->
		<stackTrace>
			exceptMessage
		</stackTrace>
	</body>
</envelope>